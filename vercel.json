{
	"version": 2,
	"builds": [
		{
			"src": "./index.js",
			"use": "@vercel/node",
			"config": {"includeFiles": ["./index.js"]}
		},
        {
            "src": "./dist/**",
            "use": "@vercel/static",
			"config": {"includeFiles": ["./dist/**"]}
        }
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "./dist/index.html"
		},
		{
			"src": "/",
			"dest": "/"
		}
	]
}